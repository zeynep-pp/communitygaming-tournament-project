type Query {
    users: [User]
}

type User {
    id: ID!
    name: String
    password: String
    email: String
    tournaments: [Tournament]
}

type Mutation {
    newUser(name: String, password: String, email: String ) : User!
    deleteUser(id: ID!) : Boolean
    updateUser(id:ID!, name: String, password: String, email: String ) : User!
}
