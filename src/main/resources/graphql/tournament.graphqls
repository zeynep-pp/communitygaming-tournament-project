type Tournament {
    tournamentName: String!
    userId: String!
}

input CreateTournamentInput {
    tournamentName: String!,
    userId: ID!,
}

extend type Query {
    tournaments(userId: ID!): [Tournament]
}

extend type Mutation {
    newTournament (tournament: CreateTournamentInput!): Tournament
    deleteTournament(id: ID!): Boolean
}